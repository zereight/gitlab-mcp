{
  "name": "@zereight/mcp-gitlab",
  "version": "2.0.6",
  "description": "MCP server for using the GitLab API",
  "license": "MIT",
  "author": "zereight",
  "type": "module",
  "bin": "./build/index.js",
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "husky",
    "dev": "npm run build && node build/index.js",
    "watch": "tsc --watch",
    "deploy": "npm publish --access public",
    "changelog": "auto-changelog -p",
    "jest": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test": "npm run jest",
    "test:unit": "npm run jest -- tests/unit",
    "test:integration": "npm run build && npm run jest -- tests/integration",
    "test:e2e": "npm run build && RUN_E2E_TESTS=true npm run jest -- tests/e2e/all-tools.test.ts",
    "test:smoke": "npm run build && RUN_E2E_TESTS=true npm run jest -- tests/e2e/all-tools.test.ts -t 'Comprehensive Tool Testing - Stdio Transport.*list all available tools' --runInBand",
    "test:server": "npm run build && node build/tests/server/test-all-transport-server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.0",
    "@types/node-fetch": "^2.6.12",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "fetch-cookie": "^3.1.0",
    "form-data": "^4.0.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^3.3.2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "socks-proxy-agent": "^8.0.5",
    "tough-cookie": "^5.1.2",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/chalk": "^0.4.31",
    "@types/commander": "^2.12.0",
    "@types/express": "^5.0.2",
    "@types/handlebars": "^4.0.40",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "auto-changelog": "^2.4.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "eslint": "^9.18.0",
    "handlebars": "^4.7.8",
    "husky": "^9.1.7",
    "inquirer": "^12.9.6",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "playwright": "^1.55.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
